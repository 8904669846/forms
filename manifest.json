{
  "manifest_version": 3,
  "name": "Quiz GPT",
  "version": "1.0",
  "description": "Solves the google form quiz by chat gpt 4",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["*://docs.google.com/forms/*"],
      "js": ["quiz-gpt.js"]
    }
  ],
  "action": {
    "default_title": "Solve this quiz"
  },
  "icons": {}
}
